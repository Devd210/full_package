<launch>
  <arg name="debug" default="false"/>
  <arg name="respawn" default="false"/>
  <arg name="gp_params" default="$(find global_planner)/config"/>

  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find global_planner)/custom_config.conf" if="$(arg debug)"/>
    
  <node name="global_planner" type="global_planner_action_node" pkg="global_planner" output="screen" respawn="$(arg respawn)">
    <rosparam command="load" file="$(arg gp_params)/global_planner_action_server_params.yaml"/>
    <!-- <rosparam command="load" file="$(find sbpl_lattice_planner)/config/sbpl_global_params.yaml"/> -->
  </node>
</launch>
